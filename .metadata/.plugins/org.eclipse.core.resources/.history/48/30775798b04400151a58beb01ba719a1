package org.ThoughtWorks.story1;

public class Quantity {

	public boolean isEqual(Meters meters, Centimeters cms) {

		if ((meters.meters) * 100 == cms.centimeters)
			return true;
		else
			return false;
	}

	public boolean isEqual(double length, double  length1) {

		if (length==length1)
			return true;
		else
			return false;
	}
	
}
